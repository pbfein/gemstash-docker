---
:base_path: "/var/lib/gemstash"
:protected_fetch: false # do not protect private gems
:db_adapter: mysql2
:db_url: "mysql2://gemstash:gemstash@host.docker.internal/gemstash"
:db_connection_options:
  :connect_timeout: 10
  :read_timeout: 5
  :reconnect: true
  :timeout: 30
:cache_type: memcached
:memcached_servers: host.docker.internal

